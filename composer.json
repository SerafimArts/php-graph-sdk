{
    "name": "serafim/graph-sdk",
    "description": "Facebook SDK for PHP compatible with PHP 8.1",
    "keywords": ["facebook", "sdk"],
    "type": "library",
    "homepage": "https://github.com/SerafimArts/php-graph-sdk",
    "license": "proprietary",
    "authors": [
        {
            "name": "Facebook",
            "homepage": "https://github.com/facebook/php-graph-sdk/contributors"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "vimeo/psalm": "^4.22",
        "phpunit/phpunit": "^9.5.18",
        "mockery/mockery": "^1.5.0",
        "guzzlehttp/guzzle": "^7.4",
        "rector/rector": "^0.12.17"
    },
    "autoload": {
        "psr-4": {
            "Facebook\\": "src"
        },
        "files": [
            "src/polyfills.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Facebook\\Tests\\": "tests"
        }
    },
    "replace": {
        "facebook/graph-sdk": ">=5.7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
